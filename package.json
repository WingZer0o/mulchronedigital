{
  "name": "node.js-angular4-starter",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Mike Mulchrone",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "concurrently \"ng serve --proxy-config proxy.conf.json\" \"webpack --watch\" \"nodemon dist/server.js\" \"mongod\"",
    "dev:client": "ng serve --proxy-config proxy.conf.json",
    "dev:server-build": "webpack --watch",
    "dev:server-watch": "nodemon dist/server.js",
    "prod:client-build": "ng build --prod --aot",
    "prod:server-build": "webpack",
    "database": "mongod",
    "postinstall": "concurrently \"ng build --prod --aot\" \"webpack\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "express": "^4.15.4",
    "font-awesome": "^4.7.0",
    "mongodb": "^2.2.31",
    "morgan": "^1.8.2",
    "request": "^2.81.0",
    "rxjs": "^5.4.2",
    "socket.io": "^2.0.3",
    "ts-loader": "^2.3.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/bcryptjs": "^2.4.1",
    "@types/bluebird": "^3.5.8",
    "@types/express": "^4.0.37",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/mongodb": "^2.2.11",
    "@types/node": "~6.0.60",
    "@types/request": "^2.0.3",
    "@types/socket.io": "^1.4.30",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
